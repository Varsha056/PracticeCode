/*
1=1
1+2=3
1+2+3=6
*/

import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();

		for(int i=1;i<=num;i++){
			int sum = 0;
			for(int j=1;j<=i;j++){
				System.out.print(j);
				sum+=j;
				if(i>1 && j<i)
				System.out.print("+");
			}
			System.out.print("="+sum);
			System.out.println();
		}
	}
}
