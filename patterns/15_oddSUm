import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int p =1;

		for(int i = 1;i<= num*2;i+=2){
			for(int j=i;j<=num*2;j+=2){
				System.out.print(j);
		}
		for(int k =1;k<=i-1;k+=2){
			System.out.print(k);
		}
		System.out.println();
		}
	}
}
