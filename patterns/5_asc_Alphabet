/*

 A
 BB
 CCC
*/
import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();

		for(int i=1;i<=num;i++){
		Character chVal=(char)( 'A'+i-1);
			for(int j=1;j<=i;j++){
				System.out.print(chVal);
			}
			System.out.println();
		}
	}

}
