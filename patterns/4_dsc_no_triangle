/*
1
21
321
4321
*/
import java.util.Scanner;

public class Solution {


	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		for(int i=1;i<=num;i++){
			int p=i;
			for(int j=1;j<=i;j++){
				System.out.print(p);
				p--;
			}
			System.out.println();
		}
	}

}
