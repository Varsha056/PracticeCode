/*
9 3 6 12 4 32
3 9 12 6 32 4
*/
public class Solution {
    
    public static void swapAlternate(int arr[]) {
    	//Your code goes here

        for(int i = 0; i<arr.length;i+=2){
            if(i+1==arr.length)
            return;
            int temp =arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=temp;
        }
    }
}
