/*
pair sum
i/p
0 4 1 2 5 4 
5
o/p
3
*/
public class Solution {  

    public static int pairSum(int arr[], int x) {
    	//Your code goes here
        int sum =0;
        for(int i = 0; i< arr.length -1;i++){
            for(int j =i+1;j<arr.length;j++){
                if(arr[i]+arr[j]==x){
                    sum++;
                }
            }
        }
        return sum;
    }
}
