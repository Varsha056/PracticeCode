public class Solution {
    public static void insertionSort(int[] arr, int size) {
        //Your code goes here

        for(int  i = 0;i<size;i++){
            int temp = arr[i];
            for( int j= i-1;j>=0;j--){
                if(temp<arr[j]){
                    arr[j+1]=arr[j];
                    arr[j]=temp;
                }
            }
        }
    }
}
