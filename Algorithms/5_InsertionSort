public class Solution {
    public static void insertionSort(int[] arr, int size) {
        //Your code goes here

        for(int  i = 0;i<size;i++){
            int temp = arr[i];
//use while loop for second loop for better understanding
            for( int j= i-1; j>=0 && temp<arr[j];j--){
                    arr[j+1]=arr[j];
                    arr[j]=temp;
            }
        }
    }
}
